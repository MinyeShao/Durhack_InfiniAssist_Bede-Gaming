{
	"easycom": {
		"^u-(.*)": "uview-ui/components/u-$1/u-$1.vue"
	},
	"pages": [
		{
			"path": "pages/main/index/index",
			"style": {
				"navigationBarTitleText": "AI Assistant",
				"enablePullDownRefresh": false,
				"navigationStyle": "custom",
				"app-plus": {
					"softinputMode": "adjustResize"
				}
			}
		},
		{
			"path": "pages/main/form/index",
			"style": {
				"navigationBarTitleText": "AI Assistant",
				"enablePullDownRefresh": false,
				"navigationBarBackgroundColor": "#26B3A0",
				"navigationBarTextStyle": "white"
			}
		},
		{
			"path": "pages/main/login/login",
			"style": {
				"navigationBarTitleText": "Login to ChatGPT",
				"enablePullDownRefresh": false
			}
		},
		{
			"path": "pages/main/admin/admin",
			"style": {
				"navigationBarTitleText": "Server Settings",
				"enablePullDownRefresh": false
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "AI Assistant",
		"navigationBarBackgroundColor": "#ffffff",
		"backgroundColor": "#ffffff"
	},
	"uniIdRouter": {}
}
